#include <iostream>
#include <string>
#include <locale.h>
#include <cstdlib>
#include <time.h>
using namespace std;



int main()
{
	setlocale(LC_ALL, "Portuguese");
	srand(time(0));
    int A, L, C;
    cin >> A >> L >> C;
    int volume = A * L * C;

    if (A >= 3 && volume >= 50) {
       
        cout << 1 << endl;
    }
    else {
       
        cout << 0 << endl;
    }

    return 0;
}
